<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="DslBottomTabItemView">
        <!--默认状态下的图标-->
        <attr name="iconNormal" format="reference" />
        <!--选中状态下的图标-->
        <attr name="iconSelected" format="reference" />

        <attr name="blackModeIconNormal" format="reference" />

        <attr name="blackModeIconSelected" format="reference" />
        //
        <!--底部文字-->
        <attr name="itemText" format="string" />
        <!--文字大小-->
        <attr name="itemTextSize" format="dimension" />
        <!--文字加粗-->
        <!--默认状态下的文字颜色-->
        <attr name="textColorNormal" format="color" />
        <!--选中状态下的文字颜色-->
        <attr name="textColorSelected" format="color" />

        <attr name="blackModeTextColorNormal" format="color" />
        <!--选中状态下的文字颜色-->
        <attr name="blackModeTextColorSelected" format="color" />
    </declare-styleable>

</resources>